{
  "name": "nikosharp",
  "displayName": "NikoSharp",
  "description": "NikoSharp Programming Language",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nikos",
